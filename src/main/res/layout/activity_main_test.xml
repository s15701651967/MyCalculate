<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <Button
        android:id="@+id/Return2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:text="返回菜单" />


    <EditText
        android:id="@+id/input"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:ems="20"
        android:inputType="textEmailAddress"
        android:text="0"
        android:textSize="20dp" />



    <Button
        android:id="@+id/C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
        android:layout_below="@+id/input"
        android:layout_toRightOf="@+id/factorial"
        android:minHeight="0dp"
        android:minWidth="120dp"/>


    <Button
        android:id="@+id/sin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/input"
        android:layout_alignParentLeft="true"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="sin" />

    <Button
        android:id="@+id/cos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/input"
        android:layout_toRightOf="@+id/sin"
        android:text="cos"
        android:minHeight="0dp"
        android:minWidth="120dp"/>

    <Button
        android:id="@+id/tan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/input"
        android:layout_toRightOf="@+id/cos"
        android:text="tan"
        android:minHeight="0dp"
        android:minWidth="120dp"/>

    <Button
        android:id="@+id/factorial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/input"
        android:layout_toRightOf="@+id/tan"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="n!" />
    <Button
        android:id="@+id/bksp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bksp"
        android:layout_below="@+id/input"
        android:layout_toRightOf="@+id/C"
        android:minHeight="0dp"
        android:minWidth="120dp"/>
    <Button
        android:id="@+id/num_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:layout_below="@+id/sin"
        android:layout_alignParentLeft="true"
        android:minWidth="120dp"
        android:minHeight="0dp"/>
    <Button
        android:id="@+id/num_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:layout_below="@+id/sin"
        android:layout_toRightOf="@+id/num_7"
        android:minWidth="120dp"
        android:minHeight="0dp"/>

    <Button
        android:id="@+id/num_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:layout_below="@+id/sin"
        android:layout_toRightOf="@+id/num_8"
        android:minWidth="120dp"
        android:minHeight="0dp"/>

    <Button
        android:id="@+id/num_divide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sin"
        android:layout_toRightOf="@+id/num_9"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="/" />

    <Button
        android:id="@+id/num_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sin"
        android:layout_toRightOf="@+id/num_divide"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="(" />

    <Button
        android:id="@+id/num_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sin"
        android:layout_toRightOf="@+id/num_left"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text=")" />
    <Button
        android:id="@+id/num_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:layout_below="@+id/num_7"
        android:layout_alignParentLeft="true"
        android:minWidth="120dp"
        android:minHeight="0dp"/>

    <Button
        android:id="@+id/num_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:layout_below="@+id/num_7"
        android:layout_toRightOf="@+id/num_7"
        android:minWidth="120dp"
        android:minHeight="0dp"/>
    <Button
        android:id="@+id/num_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:layout_below="@+id/num_7"
        android:layout_toRightOf="@+id/num_5"
        android:minWidth="120dp"
        android:minHeight="0dp"/>

    <Button
        android:id="@+id/multiply"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_7"
        android:layout_toRightOf="@+id/num_6"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="*" />

    <Button
        android:id="@+id/radical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_7"
        android:layout_toRightOf="@+id/multiply"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="√" />

    <Button
        android:id="@+id/square"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_7"
        android:layout_toRightOf="@+id/radical"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="^" />

    <Button
        android:id="@+id/num_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:layout_below="@+id/num_4"
        android:layout_alignParentLeft="true"
        android:minWidth="120dp"
        android:minHeight="0dp"/>

    <Button
        android:id="@+id/num_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_4"
        android:layout_toRightOf="@+id/num_7"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="2" />

    <Button
        android:id="@+id/num_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_4"
        android:layout_toRightOf="@+id/num_2"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="3" />

    <Button
        android:id="@+id/minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_4"
        android:layout_toRightOf="@+id/num_3"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="-" />
    <Button
        android:id="@+id/log"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="log"
        android:layout_below="@+id/num_4"
        android:layout_toRightOf="@+id/minus"
        android:minWidth="120dp"
        android:minHeight="0dp"/>
    <Button
        android:id="@+id/ln"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ln"
        android:layout_below="@+id/num_4"
        android:layout_toRightOf="@+id/log"
        android:minWidth="120dp"
        android:minHeight="0dp"/>

    <Button
        android:id="@+id/num_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_1"
        android:layout_alignParentLeft="true"
        android:minWidth="120dp"
        android:minHeight="0dp"
        android:text="0" />
    <Button
        android:id="@+id/point"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="."
        android:layout_below="@+id/num_1"
        android:layout_toRightOf="@+id/num_7"
        android:minWidth="120dp"
        android:minHeight="0dp"/>
    <Button
        android:id="@+id/equal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="="
        android:layout_below="@+id/num_1"
        android:layout_toRightOf="@+id/num_2"
        android:minWidth="120dp"
        android:minHeight="0dp"/>
    <Button
        android:id="@+id/add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+"
        android:layout_below="@+id/num_1"
        android:layout_toRightOf="@+id/num_3"
        android:minWidth="120dp"
        android:minHeight="0dp"/>

    <Button
        android:id="@+id/exit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_1"
        android:layout_toRightOf="@+id/add"
        android:minWidth="240dp"
        android:minHeight="0dp"
        android:text="exit" />

    <TextView
        android:id="@+id/tip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/num_0"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="提示：欢迎使用！" />

</RelativeLayout>